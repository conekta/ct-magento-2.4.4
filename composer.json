{
    "name": "conekta/conekta_payments",
    "description": "Conekta payment gateway",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "require": {
        "php": "~8.1",
        "magento/module-sales": "^103.0.4",
        "magento/module-checkout": "^100.4.4",
        "magento/module-payment": "^100.4.4",
        "magento/framework": "^103.0.4",
        "conekta/conekta-php": "~4.2.0",
        "monolog/monolog": "^2.6.0"
    },
    "type": "magento2-module",
    "version": "5.0.1",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "autoload": {
        "files": [
          "registration.php"
        ],
        "psr-4": {
            "Conekta\\Payments\\": ""
        }
    },
    "config": {
        "allow-plugins": {
            "magento/composer-dependency-version-audit-plugin": true
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.7"
    }
}
